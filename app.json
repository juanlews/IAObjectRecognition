{
  "name": "pose-detection-expo",
  "displayName": "pose-detection-expo",
  "expo": {
    "name": "pose-detection-expo",
    "slug": "pose-detection-expo",
    "version": "1.0.0",
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ],
      [
        "expo-image-picker",
        
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ]
  }
}
